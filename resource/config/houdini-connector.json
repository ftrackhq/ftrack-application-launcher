{
    "priority":100,
    "context": ["Task"],
    "identifier": "ftrack-connect-launch-houdini",
    "applicationIdentifier":"houdini_{version}",
    "integrations": {
        "connector":["ftrack-connect-houdini"]
    },
    "label": "Houdini",
    "icon": "houdini",
    "variant": "{version}",
    "search_path":{
        "linux": {
            "prefix":["/", "opt"],
            "expression":["hfs*", "bin", "houdinifx-bin"]
        },
        "windows": {
            "prefix":["C:\\", "Program Files.*"],
            "expression":["Side Effects Software", "Houdini*", "bin", "houdini.exe"]
        },
        "darwin": {
            "prefix":["/", "Applications"],
            "expression": ["Apprentice", "Core", "FX", "Indie", "Houdini", "Houdini.*",  "Houdini *\\.app"]
        }
    }
 }
 