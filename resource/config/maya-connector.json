{
    "priority":100,
    "context": ["Task"],
    "identifier": "ftrack-connect-launch-maya",
    "applicationIdentifier":"maya_{version}",
    "integrations": {
        "connector":["ftrack-connect-maya"]
    },
    "label": "Maya",
    "icon": "maya",
    "variant": "{version}",
    "search_path":{
        "linux": {
            "prefix":["/", "usr","autodesk","maya.+"],
            "expressions":[["bin","maya$"]],
            "version_expression": "maya(?P<version>\\d{4})"
        },
        "windows": {
            "prefix":["C:\\", "Program Files.*"],
            "expressions":[["Autodesk", "Maya.+", "bin", "maya.exe"]]
        },
        "darwin": {
            "prefix":["/", "Applications"],
            "expressions": [["Autodesk", "maya.+", "Maya.app"]]
        }
    }
 }
 